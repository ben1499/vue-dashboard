{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\n// import SuccessModal from './SuccessModal.vue';\n\nexport default {\n  props: {\n    id: null\n  },\n  data() {\n    return {\n      productData: {},\n      isDelete: false,\n      fullScreenLoading: false\n    };\n  },\n  mounted() {\n    this.showProductDetails();\n  },\n  methods: {\n    showProductDetails() {\n      const loading = this.$loading({\n        lock: true,\n        text: 'Loading',\n        spinner: 'el-icon-loading',\n        background: 'rgba(0, 0, 0, 0.7)'\n      });\n      axios.get(`https://dummyjson.com/products/${this.id}`).then(response => {\n        loading.close();\n        this.productData = response.data;\n      }).catch(err => console.log(err));\n    },\n    handleDelete() {\n      this.$confirm('This will permanently delete the file. Continue?', 'Warning', {\n        confirmButtonText: 'OK',\n        cancelButtonText: 'Cancel',\n        type: 'warning'\n      }).then(() => {\n        this.isDelete = true;\n        axios.delete(`https://dummyjson.com/products/${this.id}`).then(response => {\n          console.log(response);\n          this.isDelete = false;\n          this.$router.push('/');\n        });\n        this.$message({\n          type: 'success',\n          message: 'Delete completed'\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: 'Delete canceled'\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","props","id","data","productData","isDelete","fullScreenLoading","mounted","showProductDetails","methods","loading","$loading","lock","text","spinner","background","get","then","response","close","catch","err","console","log","handleDelete","$confirm","confirmButtonText","cancelButtonText","type","delete","$router","push","$message","message"],"sources":["src/components/ProductItem.vue"],"sourcesContent":["<template>\n  <div id=\"item\" v-loading.fullscreen.lock=\"fullScreenLoading\">\n    <router-link to=\"/\">\n      <el-button>Go back</el-button>\n    </router-link>\n    <div>\n      <div>\n          <img :src=\"productData.images[0]\" alt=\"\">\n      </div>\n      <div class=\"details\">\n          <p>{{ productData.brand }}</p>\n          <h1>{{ productData.title }} </h1>\n          <p>{{ productData.description }}</p>\n          <h2>$ {{ productData.price }}</h2>\n          <p>Items in stock: {{ productData.stock }}</p>\n          <p>Rating: {{ productData.rating }} out of 5</p>\n          <p>Category: {{ productData.category }}</p>\n          <div class=\"buttons\">\n            <router-link :to=\"{name: 'edit', params: {id: id}}\">\n              <el-button type=\"primary\">Edit</el-button>\n            </router-link>\n            <el-button @click=\"handleDelete\" :loading=\"isDelete\" type=\"danger\">Delete</el-button>\n          </div>\n      </div>\n    </div>\n    <!-- <success-modal :process=\"'Deleted'\" v-if=\"showModal\" class=\"success\"></success-modal> -->\n    <!-- <edit-modal @submit-click=\"showEditModal = !showEditModal\" @cancel-click=\"showEditModal = !showEditModal\" v-if=\"showEditModal\" :id=\"id\"></edit-modal> -->\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n// import SuccessModal from './SuccessModal.vue';\n\n\nexport default {\n  props: {\n    id: null\n  },\n  data() {\n    return {\n      productData: {},\n      isDelete: false,\n      fullScreenLoading: false,\n    }\n  },\n  mounted() {\n    this.showProductDetails();\n  },\n  methods: {\n    showProductDetails() {\n      const loading = this.$loading({\n        lock: true,\n        text: 'Loading',\n        spinner: 'el-icon-loading',\n        background: 'rgba(0, 0, 0, 0.7)'\n      });\n      axios.get(`https://dummyjson.com/products/${this.id}`)\n      .then((response) => {\n        loading.close()\n        this.productData = response.data;\n      })\n      .catch(err => console.log(err))\n    },\n    handleDelete() {\n      this.$confirm('This will permanently delete the file. Continue?', 'Warning', {\n          confirmButtonText: 'OK',\n          cancelButtonText: 'Cancel',\n          type: 'warning'\n        }).then(() => {\n          this.isDelete = true;\n          axios.delete(`https://dummyjson.com/products/${this.id}`).then((response) => {\n            console.log(response);\n            this.isDelete = false;\n            this.$router.push('/');\n          })\n          this.$message({\n            type: 'success',\n            message: 'Delete completed'\n          });\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: 'Delete canceled'\n          });          \n        });\n    },\n\n  }\n}\n</script>\n\n<style scoped>\n#item {\n  margin-left: 50px;\n  margin-top: 30px;\n}\n\n#item::not(.success) {\n  -webkit-filter: blur(8px);\n    -ms-filter: blur(8px);\n    -moz-filter: blur(8px);\n    -o-filter: blur(8px);\n    filter: blur(8px);\n}\n\n#item > div:first-of-type {\n  display: flex;\n  gap: 80px;\n  margin-top: 20px;\n}\n\nimg {\n  width: 450px;\n  height: auto;\n}\n\n.details > p:nth-of-type(2) {\n  width: 500px;\n  margin-bottom: 30px;\n  color: #585555;\n}\n\n.details > p:nth-of-type(3) {\n  margin-top: 20px;\n}\n\n.buttons {\n  margin-top: 20px;\n}\n\na {\n  margin-right: 20px;\n}\n</style>"],"mappings":";AA+BA,OAAAA,KAAA;AACA;;AAGA;EACAC,KAAA;IACAC,EAAA;EACA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,QAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,kBAAA;EACA;EACAC,OAAA;IACAD,mBAAA;MACA,MAAAE,OAAA,QAAAC,QAAA;QACAC,IAAA;QACAC,IAAA;QACAC,OAAA;QACAC,UAAA;MACA;MACAf,KAAA,CAAAgB,GAAA,wCAAAd,EAAA,IACAe,IAAA,CAAAC,QAAA;QACAR,OAAA,CAAAS,KAAA;QACA,KAAAf,WAAA,GAAAc,QAAA,CAAAf,IAAA;MACA,GACAiB,KAAA,CAAAC,GAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;IACA;IACAG,aAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAX,IAAA;QACA,KAAAZ,QAAA;QACAL,KAAA,CAAA6B,MAAA,wCAAA3B,EAAA,IAAAe,IAAA,CAAAC,QAAA;UACAI,OAAA,CAAAC,GAAA,CAAA