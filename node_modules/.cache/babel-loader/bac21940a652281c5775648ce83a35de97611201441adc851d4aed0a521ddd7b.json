{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport SuccessModal from './SuccessModal.vue';\nexport default {\n  props: ['id'],\n  data() {\n    return {\n      productData: {},\n      isDelete: false,\n      showModal: false,\n      showEditModal: false\n    };\n  },\n  components: {\n    SuccessModal\n  },\n  mounted() {\n    this.showProductDetails();\n  },\n  methods: {\n    showProductDetails() {\n      axios.get(`https://dummyjson.com/products/${this.id}`).then(response => {\n        this.productData = response.data;\n      });\n    },\n    handleDelete() {\n      this.$confirm('This will permanently delete the file. Continue?', 'Warning', {\n        confirmButtonText: 'OK',\n        cancelButtonText: 'Cancel',\n        type: 'warning'\n      }).then(() => {\n        this.isDelete = true;\n        this.showModal = true;\n        axios.delete(`https://dummyjson.com/products/${this.id}`).then(response => {\n          this.showModal = false;\n          console.log(response);\n          this.isDelete = false;\n          this.$router.push('/');\n        });\n        this.$message({\n          type: 'success',\n          message: 'Delete completed'\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: 'Delete canceled'\n        });\n      });\n    }\n    // handleEdit() {\n    //   this.showEditModal = true;\n    // }\n  }\n};","map":{"version":3,"names":["axios","SuccessModal","props","data","productData","isDelete","showModal","showEditModal","components","mounted","showProductDetails","methods","get","id","then","response","handleDelete","$confirm","confirmButtonText","cancelButtonText","type","delete","console","log","$router","push","$message","message","catch"],"sources":["src/components/ProductItem.vue"],"sourcesContent":["<template>\n  <div id=\"item\">\n    <router-link to=\"/\">\n      <el-button>Go back</el-button>\n    </router-link>\n    <div>\n      <div>\n          <img :src=\"productData.images[0]\" alt=\"\">\n      </div>\n      <div class=\"details\">\n          <p>{{ productData.brand }}</p>\n          <h1>{{ productData.title }} </h1>\n          <p>{{ productData.description }}</p>\n          <h2>$ {{ productData.price }}</h2>\n          <p>Items in stock: {{ productData.stock }}</p>\n          <p>Rating: {{ productData.rating }} out of 5</p>\n          <p>Category: {{ productData.category }}</p>\n          <div class=\"buttons\">\n            <router-link :to=\"{name: 'edit', params: {id: id}}\">\n              <el-button @click=\"handleEdit\" type=\"primary\">Edit</el-button>\n            </router-link>\n            <el-button @click=\"handleDelete\" :loading=\"isDelete\" type=\"danger\">Delete</el-button>\n          </div>\n      </div>\n    </div>\n    <success-modal :process=\"'Deleted'\" v-if=\"showModal\" class=\"success\"></success-modal>\n    <!-- <edit-modal @submit-click=\"showEditModal = !showEditModal\" @cancel-click=\"showEditModal = !showEditModal\" v-if=\"showEditModal\" :id=\"id\"></edit-modal> -->\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport SuccessModal from './SuccessModal.vue';\n\n\nexport default {\n  props: ['id'],\n  data() {\n    return {\n      productData: {},\n      isDelete: false,\n      showModal: false,\n      showEditModal: false,\n    }\n  },\n  components: {\n    SuccessModal,\n  },\n  mounted() {\n    this.showProductDetails();\n  },\n  methods: {\n    showProductDetails() {\n      axios.get(`https://dummyjson.com/products/${this.id}`).then((response) => {\n        this.productData = response.data;\n      })\n    },\n    handleDelete() {\n      this.$confirm('This will permanently delete the file. Continue?', 'Warning', {\n          confirmButtonText: 'OK',\n          cancelButtonText: 'Cancel',\n          type: 'warning'\n        }).then(() => {\n          this.isDelete = true;\n          this.showModal = true;\n          axios.delete(`https://dummyjson.com/products/${this.id}`).then((response) => {\n          this.showModal = false;\n          console.log(response);\n          this.isDelete = false;\n          this.$router.push('/');\n        })\n          this.$message({\n            type: 'success',\n            message: 'Delete completed'\n          });\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: 'Delete canceled'\n          });          \n        });\n\n\n    },\n    // handleEdit() {\n    //   this.showEditModal = true;\n    // }\n  }\n}\n</script>\n\n<style scoped>\n#item {\n  margin-left: 50px;\n  margin-top: 30px;\n}\n\n#item::not(.success) {\n  -webkit-filter: blur(8px);\n    -ms-filter: blur(8px);\n    -moz-filter: blur(8px);\n    -o-filter: blur(8px);\n    filter: blur(8px);\n}\n\n#item > div:first-of-type {\n  display: flex;\n  gap: 80px;\n  margin-top: 20px;\n}\n\nimg {\n  width: 450px;\n  height: auto;\n}\n\n.details > p:nth-of-type(2) {\n  width: 500px;\n  margin-bottom: 30px;\n  color: #585555;\n}\n\n.details > p:nth-of-type(3) {\n  margin-top: 20px;\n}\n\n.buttons {\n  margin-top: 20px;\n}\n\na {\n  margin-right: 20px;\n}\n</style>"],"mappings":";AA+BA,OAAAA,KAAA;AACA,OAAAC,YAAA;AAGA;EACAC,KAAA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,QAAA;MACAC,SAAA;MACAC,aAAA;IACA;EACA;EACAC,UAAA;IACAP;EACA;EACAQ,QAAA;IACA,KAAAC,kBAAA;EACA;EACAC,OAAA;IACAD,mBAAA;MACAV,KAAA,CAAAY,GAAA,wCAAAC,EAAA,IAAAC,IAAA,CAAAC,QAAA;QACA,KAAAX,WAAA,GAAAW,QAAA,CAAAZ,IAAA;MACA;IACA;IACAa,aAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAN,IAAA;QACA,KAAAT,QAAA;QACA,KAAAC,SAAA;QACAN,KAAA,CAAAqB,MAAA,wCAAAR,EAAA,IAAAC,IAAA,CAAAC,QAAA;UACA,KAAAT,SAAA;UACAgB,OAAA,CAAAC,GAAA,CAAAR,QAAA;UACA,KAAAV,QAAA;UACA,KAAAmB,OAAA,CAAAC,IAAA;QACA;QACA,KAAAC,QAAA;UACAN,IAAA;UACAO,OAAA;QACA;MACA,GAAAC,KAAA;QACA,KAAAF,QAAA;UACAN,IAAA;UACAO,OAAA;QACA;MACA;IAGA;IACA;IACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}