{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'FilterComponent',\n  data() {\n    return {\n      categories: [],\n      checkList: []\n    };\n  },\n  mounted() {\n    this.getCategories();\n  },\n  methods: {\n    getCategories() {\n      axios.get(`https://dummyjson.com/products/categories`).then(response => {\n        this.categories = response.data.slice(0, 6);\n      }).catch(err => console.log(err));\n    },\n    sendCheckList() {\n      console.log(this.checkList);\n      this.$emit('filter-change', this.checkList);\n      console.log(\"Emitted\");\n    },\n    handleReset() {\n      this.checkList = [];\n      this.$emit('filter-change', this.checkList);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","categories","checkList","mounted","getCategories","methods","get","then","response","slice","catch","err","console","log","sendCheckList","$emit","handleReset"],"sources":["src/components/Controls/FilterComponent.vue"],"sourcesContent":["<template>\n    <div>\n        <el-dropdown>\n            <el-button type=\"primary\">\n                Filter<i class=\"el-icon-arrow-down el-icon--right\"></i>\n            </el-button>\n            <el-dropdown-menu slot=\"dropdown\">\n                <!-- <el-dropdown-item v-for=\"(category, index) in categories\" :key=\"index\">{{ category }}</el-dropdown-item> -->\n                <el-dropdown-item v-for=\"(category, index) in categories\" :key=\"index\">\n                    <el-checkbox-group v-model=\"checkList\" @change=\"sendCheckList\">\n                        <el-checkbox :label=\"category\"></el-checkbox>\n                    </el-checkbox-group>\n                </el-dropdown-item>\n                <el-dropdown-item><li @click=\"handleReset\">Reset</li></el-dropdown-item>\n            </el-dropdown-menu>\n        </el-dropdown>\n    </div>\n\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n    name: 'FilterComponent',\n    data() {\n        return {\n            categories: [],\n            checkList: []\n        }\n    },\n    mounted() {\n        this.getCategories();\n    },\n    methods: {\n        getCategories() {\n            axios.get(`https://dummyjson.com/products/categories`).then((response) => {\n                this.categories = response.data.slice(0, 6);\n            }).catch(err => console.log(err))\n        }, \n        sendCheckList() {\n            console.log(this.checkList)\n            this.$emit('filter-change', this.checkList);\n            console.log(\"Emitted\");\n        },\n        handleReset() {\n            this.checkList = [];\n            this.$emit('filter-change', this.checkList)\n        }\n    }\n}\n</script>\n\n<style scoped>\n#filter {\n    margin-left: 50px;\n}\n</style>"],"mappings":"AAqBA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACAD,cAAA;MACAN,KAAA,CAAAQ,GAAA,8CAAAC,IAAA,CAAAC,QAAA;QACA,KAAAP,UAAA,GAAAO,QAAA,CAAAR,IAAA,CAAAS,KAAA;MACA,GAAAC,KAAA,CAAAC,GAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;IACA;IACAG,cAAA;MACAF,OAAA,CAAAC,GAAA,MAAAX,SAAA;MACA,KAAAa,KAAA,uBAAAb,SAAA;MACAU,OAAA,CAAAC,GAAA;IACA;IACAG,YAAA;MACA,KAAAd,SAAA;MACA,KAAAa,KAAA,uBAAAb,SAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}